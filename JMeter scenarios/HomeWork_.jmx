            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">import org.apache.commons.lang3.RandomStringUtils

// Функция для генерации случайного username
def genUsername(int length) {
    return RandomStringUtils.randomAlphabetic(length)
}

// Функция для генерации случайного password
def genPassword(int length) {
    return RandomStringUtils.randomAlphanumeric(length)
}

// Функция для регулирования длины переменных
def usernameLength = 8
def passwordLength = 10

// Основная часть программы
def username = genUsername(usernameLength)
def password = genPassword(passwordLength)

// Сохранение переменных в контекст JMeter
vars.put(&quot;username&quot;, username)
vars.put(&quot;password&quot;, password)

// Сохранение в файл
def file = new File(&apos;C:/Users/kiril/JMeter scenarios/users.txt&apos;)
if (!file.exists()) {
    file.createNewFile()
    file.append(&quot;username,password&quot;)
}
file.append(&quot;\n${username},${password}&quot;)

//Запись в логах о создании пользователя
log.info(&quot;Создан новый пользователь: Username: $username, Password: $password&quot;)</stringProp>
