        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="UC01_T02_write_txt">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">// Определяем размер массива строк
def size = 16

// Создаем массив строк нужного размера
def arrStr = new String[size]

// Заполняем массив строк переменными по счетчику
for (int i = 0; i &lt; size; i++) {
    arrStr[i] = vars.get(&quot;content_${i + 1}&quot;)
}

// Создаем массив с длиной элементов массива строк
def lengths = arrStr.collect { it.length() }

// Формируем итоговую строку для записи в файл
StringBuilder output = new StringBuilder()
for (int i = 0; i &lt; arrStr.size(); i++) {
    output.append(&quot;${i + 1} : ${arrStr[i]} : ${lengths[i]}\n&quot;)
}

// Записываем результат в файл
File file = new File(&quot;C:/Users/kiril/JMeter scenarios/RegEx.txt&quot;)
file.write(output.toString())</stringProp>
        </JSR223Sampler>
