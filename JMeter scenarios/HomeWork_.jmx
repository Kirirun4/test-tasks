            <stringProp name="BeanShellSampler.query">import org.apache.commons.lang.RandomStringUtils;

if (&quot;${roundTripTicket}&quot; == &quot;random&quot;){
	String randomPercentage = RandomStringUtils.randomNumeric(2);
	int percentage = Integer.parseInt(randomPercentage);

	if (percentage &gt; 75) {
    		vars.put(&quot;Branch&quot;, &quot;1&quot;);
	} else {
    		vars.put(&quot;Branch&quot;, &quot;2&quot;);
	}

	log.info(&quot;-----Сгенерирован randomPercentage: &quot; + randomPercentage);
	
} else if (&quot;${roundTripTicket}&quot; == &quot;off&quot;) {
    	vars.put(&quot;Branch&quot;, &quot;1&quot;);
} else if (&quot;${roundTripTicket}&quot; == &quot;on&quot;) {
    	vars.put(&quot;Branch&quot;, &quot;2&quot;);
} else {
	log.info(&quot;----Переменная roundtripTicket не соответсвует ожиданию - &quot; + roundtripTicket);
}</stringProp>
          </BeanShellSampler>
